            <h4>Send us a message</h4>

            <p>We pride ourselves in providing you the highest quality of professional service. Please contact us with any questions, concerns or suggestions you may have.</p>
            <p>To contact us, please call, email or complete the form below. We look forward to talking to you!</p>

            <div class="small-bar"></div>

            <!-- Contact Form -->
            <form action="@Url.Action("Contact", "Home")" method="post" id="contact-form" class="form">

                <div>
                    <label for="sender-name"><strong>Name</strong> <span>(required)</span></label>
                   @* <input name="Name" type="text" value="" id="sender-name" required="">*@
                   
                    @Html.ValidationMessageFor(m => m.Name)
                    @Html.TextBoxFor(m => m.Name, new  {autofocus="" })
                   
                </div>

                <div>
                    <label for="sender-email"><strong>E-mail</strong> <span>(required)</span></label>
                     @Html.ValidationMessageFor(m => m.Email)
                    @Html.EditorFor(m => m.Email)
                </div>

                <div>
                    <label><strong>Subject</strong> <span>(required)</span></label>
                     @Html.ValidationMessageFor(m => m.Subject)
                    @Html.TextBoxFor(m => m.Subject)
                </div>

                <div>
                    <label><strong>Message</strong> <span>(required)</span></label>
                     @Html.ValidationMessageFor(m => m.Message)
                    @Html.TextAreaFor(m => m.Message, 10, 75, new { })
                    
                </div>

                <div class="hidden">
                    <label for="spam-check"><strong>Leave this field blank:</strong></label>
                    <input name="UserName" type="text" value="" id="spam-check">
                </div>

                <div id="submit-button">
                    <input name="submit" type="submit" value="Send Message" class="button">
                    <img id="loading" src="@Url.Content("~/Content/marketing/images/icons/ajax-loader.gif")" alt="Loading..." style="width: 16px; height: 11px; display:none; ">
                </div>

                <div class="message-box short" style="display: none;"></div>
            </form>
            <!-- end Contact Form -->

        </div>
        <!-- end twelve columns -->

    </section>
    <!-- end inner grid -->

</div>

@section scripts{
    <script>
        $(function () {

            waldencpb.contact.init();
            
        });
    </script>
}

