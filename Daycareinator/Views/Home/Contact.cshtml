@model Daycareinator.Models.Home.ContactForm
@{
    ViewBag.Title = "Contact";
    Layout = "~/Views/Shared/_MarketingLayout.cshtml";
   
}

<div class="sixteen columns">
    <div class="page-title">
        <h1>Contact Us</h1>
    </div>
</div>

<div class="four columns" id="iBoard" data-smallscreen="yes">
    <!-- show info board on a small-screen mobile device: "yes" or "no" -->

    <!-- Info Text: Contact Information -->
    <div class="five columns alpha omega">
        <aside id="info-board" class="shadow" data-smallscreen="yes">
            <h5>Contact Information</h5>

            <div class="small-bar colored"></div>
            <h6 class="margin-bottom-5px">Walden Payroll and Bookkeeping</h6>
            <p>
                129 Azle Hwy<br />
                Weatherford, TX 76086
            </p>
            <ul class="contact-info">
                <li class="phone">(817) 901-4004 </li>
                <li class="email"><a href="mailto:don@waldencpb.com">don@waldencpb.com</a></li>

            </ul>

        </aside>
    </div>

</div>

